<% @books.each do |book| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= book.title %></h1>
    <i><%= book.author %></i><br />
    <p><%= book.genre %></p><br />
    <p><%= book.publisher %></p><br />
    <p><%= book.types %></p><br />
    <p><%= book.year %></p><br />
    <p><%= book.isbn %></p><br />
  </div>
<% end %>